<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fms.service.tuopu.ControlPropertyService">


    <insert id="addControlProperty" parameterType="com.fms.domain.tuopu.ControlProperty" >
        INSERT INTO t_control_property (controlId,propertyChinese,propertyEnglish,dataType,dicName,dicInfo,isDic)
        VALUES (
        #{controlId},
        #{propertyChinese},
        #{propertyEnglish},
        #{dataType},
        #{dicName},
        #{dicInfo},
        #{isDic}

        )
    </insert>

